<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android"
  id="com.ict.yimingyu-appList"
    version="0.0.1">
    <name>appList</name>
    <description>get installed app list</description>
    <author>mingyuyi</author>
    <license>Apache 2.0 License</license>
    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>
    <js-module src="www/appList.js" name="AppList">
        <clobbers target="AppList" />
    </js-module>
    <platform name="android">
        <source-file src="src/android/AppList.java" target-dir="src/com/ict/yimingyu" />        
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="AppList">
                <param name="android-package" value="com.ict.yimingyu.AppList"/>
            </feature>
        </config-file>
      <config-file target="AndroidManifest.xml" parent="/manifest">
      </config-file>
    </platform>
</plugin>